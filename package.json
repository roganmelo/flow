{
  "name": "flow",
  "private": true,
  "version": "0.0.1",
  "main": "./dist/flow.cjs",
  "module": "./dist/flow.mjs",
  "types": "./dist/flow.d.ts",
  "exports": {
    ".": {
      "require": "./dist/flow.cjs",
      "import": "./dist/flow.mjs"
    }
  },
  "engines": {
    "yarn": "You should use only NPM on this project",
    "npm": ">= 8.0.0"
  },
  "scripts": {
    "prepare": "husky install && husky set .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky",
    "start": "vite --host",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "check-style": "prettier --check .",
    "write-style": "prettier --write .",
    "check-type": "bash -c tsc-files --noEmit",
    "format": "npm run lint && npm run write-style && npm run check-type",
    "test": "vitest --passWithNoTests",
    "test-ui": "vitest --ui",
    "test-coverage": "vitest run --coverage",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/ui": "^0.29.2",
    "dts-bundle-generator": "^7.2.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  }
}
